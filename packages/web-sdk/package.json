{
  "name": "@tinycloudlabs/web-sdk",
  "version": "0.1.2",
  "description": "A set of tools and utilities to help you build your app with TinyCloud.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:TinyCloudLabs/web-sdk.git"
  },
  "type": "commonjs",
  "author": "TinyCloud, Inc.",
  "license": "Apache-2.0 OR MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --watch",
    "watch": "webpack --watch",
    "doc": "bun run doc:extractor && bun run doc:documenter",
    "doc:extractor": "api-extractor run --local --diagnostics",
    "doc:documenter": "api-documenter markdown -i temp -o ../../documentation/docs/web-sdk/api",
    "lint": "eslint --ext .ts src/ --fix",
    "clean": "rimraf dist && rimraf temp",
    "test": "jest"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@tinycloudlabs/web-core": "0.1.2",
    "@tinycloudlabs/web-sdk-wasm": "0.1.2",
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "browser": "^0.2.6",
    "buffer": "^6.0.3",
    "cross-env": "5.2.1",
    "ethers": "^5.7.2",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "siwe": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@microsoft/api-documenter": "^7.26.5",
    "@microsoft/api-extractor": "^7.49.1",
    "@types/jest": "^27.5.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^18.19.74",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@webpack-cli/generators": "^3.0.7",
    "babel-jest": "^28.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "gh-pages": "^6.3.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "util": "^0.12.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0"
  },
  "files": [
    "dist"
  ]
}
